    	System.out.println("Do You like to play games? Yes or No");
    	Scanner console = new Scanner(System.in);		
    	String one= console.nextLine();
        if (one.equals("Yes")){
        	typeOne++;
        	}
        else {
        	typeOne--;}
        		
        System.out.println("Do You like sport? Yes or No");
        String two=console.nextLine();
        if (one.equals("Yes")){
        	typeTwo+=2;}
        else {
        	typeOne++;
        	typeTwo--;}
        
       if (typeOne>typeTwo){
    	   System.out.println("You are genius");
       }
       else{
    	   System.out.println("You are dumb");
       }
       
        
        
        
