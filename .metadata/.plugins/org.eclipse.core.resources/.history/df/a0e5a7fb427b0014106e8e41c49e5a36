public class Movie {
    double date;
    double rate;
    String name;
    String type;
    

	public Movie(double initdata,double initrate, String ininame, String inittype) {
		
		this.date=initdata;
		this.rate=initrate;
		this.name = ininame;
		this.type=inittype;
		
		

		}


}
	
